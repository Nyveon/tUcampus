{
    "name": "tUcampus",
    "version": "1.0.0",
    "manifest_version": 2,
    "permissions": [
        "storage",
        "webRequest",
        "webRequestBlocking",
        "*://ucampus.uchile.cl/*"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_popup": "html/menu.html",
        "default_title": "tUcampus"
    }
}